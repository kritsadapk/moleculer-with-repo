{
  "name": "moleculer-with-repo",
  "version": "1.0.0",
  "description": "Moleculer microservices with repository pattern",
  "main": "index.js",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.ts",
    "start": "moleculer-runner --instances services/**/*.service.ts",
    "cli": "moleculer connect NATS",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "moleculer",
    "microservices",
    "typescript",
    "repository-pattern"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "moleculer": "^0.14.25",
    "moleculer-db": "^0.8.0",
    "moleculer-db-adapter-mongoose": "^0.2.0",
    "moleculer-repl": "^0.7.0",
    "moleculer-web": "^0.10.0",
    "mongoose": "^7.6.3",
    "nats": "^2.16.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
